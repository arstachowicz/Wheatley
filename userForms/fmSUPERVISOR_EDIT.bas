Private Sub listBoxEmpty(lstBox As ListBox)
'clear all of listbox
Dim i As Long

    
    With lstBox
        i = .ListCount - 1
        Do Until i = -1
            .Selected(i) = False
            i = i - 1
        Loop
    End With
                                            
End Sub

Private Sub Form_Load()
Dim listSUP As ListBox
Set listSUP = Me.listSUP

    Call listBoxEmpty(listSUP)
    
End Sub

Private Sub listSUP_AfterUpdate()

    Dim db As DAO.Database
    Dim qdf As DAO.QueryDef
    Dim rs1 As DAO.Recordset
    Dim rs As DAO.Recordset
    
    Dim i As Long
    Dim j As Long
    Dim iArray As Long
    Dim k As Long
    Dim sSQL As String
    Dim qdfName As String
    Dim sTaskList As String
    Dim listIDs() As String
    Dim listBoxName As ListBox
    
    Set db = CurrentDb
    Set rs = db.OpenRecordset("tblSUPERVISORS")
    Set listBoxName = Me!lstTASKS
    
    lID = Nz(listSUP.Value, 0)
    qdfName = "qrySUPERVISOR_TASK"
    sTaskList = ""

    If lID <> 0 Then
    
        'pull table information
        With rs
            .MoveFirst
            'loop through recordset until ID matches, then pull entered values and store in txt
            Do While Not .EOF
                If !ID = lID Then
                    txtSUP1 = Nz(!firstName)
                    txtSUP2 = Nz(!lastName)
                    txtSHIFT = Nz(!Shift)
                    txtEMAIL = Nz(!email)
                    txtUSER = Nz(!username)
                    txtPASS = Nz(!password)
                    tglEMPLOY = !Employed
                    Call tglEMPLOY_Click
                    Call listBoxEmpty(listBoxName)
    
                    'List box selector
                        sSQL = "SELECT tblSUPERVISORS.ID, tblTASKLIST.ID FROM tblSUPERVISORS " & vbCr & _
                            "INNER JOIN tblTASKLIST ON tblSUPERVISORS.ID = tblTASKLIST.supervisorResponsible.Value " & vbCr & _
                            "WHERE (((tblSUPERVISORS.ID)=" & lID & "));"
                            
                        Set qdf = db.QueryDefs(qdfName)
                        qdf.SQL = sSQL
                        Set rs1 = qdf.OpenRecordset

                        With rs1
                            iArray = .RecordCount
                            If iArray > 0 Then
                                ReDim listIDs(0 To iArray)
                            'build an array for the multi-select list box
                                .MoveFirst
                                Do Until .EOF
                                    listIDs(i) = ![tblTASKLIST.ID]
                                    .MoveNext
                                    i = i + 1
                                Loop
                        
                                .Close
                                qdf.Close
                                
                                'loop through listbox and select lines that match those in array
                                If ((UBound(listIDs()) - LBound(listIDs())) > 0) Then
                                    With lstTASKS
                                        .SetFocus
                                        For j = 0 To UBound(listIDs)
                                            i = .ListCount
                                            'select only values in listbox that match the current tasks assigned
                                            'do backwards so listbox finishes with scrollbar up at top
                                            Do Until (i = -1)
                                                If .Column(0, i) = listIDs(j) Then
                                                    .Selected(i) = True
                                                End If
                                                i = i - 1
                                            Loop
                                        Next j
                                    End With
                                End If
                            
                            End If
                        End With
                    
                    
                Exit Do
                End If
                .MoveNext
            Loop
            .Close
        End With
        
    End If

    db.Close
    
End Sub

