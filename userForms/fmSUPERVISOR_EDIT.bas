Private Sub cmdENTER_Click()
Dim db As DAO.Database
Dim sSQL As String

Dim sP As String
Dim sP1 As String
Dim tbl As String
Dim sEm As String


    tbl = "tblSUPERVISORS"

    sP = ", "
    sP1 = "', "
    
    If tglEMPLOY = True Then
        sEm = "True"
    Else:
        sEm = "False"
    End If
    
    Set db = CurrentDb

'update the supervisor table on click of button
        sSQL = "Update " & tbl & vbCr & _
                " SET [firstName] = '" & txtSUP1 & sP1 & _
                    "[lastName] = '" & txtSUP2 & sP1 & _
                    "[shift] = " & Nz(txtSHIFT, 0) & sP & _
                    "[email] = '" & txtEMAIL & sP1 & _
                    "[username] = '" & txtUSER & sP1 & _
                    "[password] = '" & txtPASS & sP1 & _
                    "[employed] = " & sEm & vbCr & _
                " WHERE [ID] = " & lID & ";"
        Call taskListGen 'loops through respons. records
        
    With db
        .Execute sSQL, dbFailOnError
        .Close
    End With
    
    MsgBox "Successfully updated!"
    lstTASKS.Requery
    listSUP.Requery
    
End Sub
Private Sub taskListGen()

Dim tbl As String
Dim taskID As Long
Dim i As Long
Dim bSel As Boolean


tbl = "tblTASKLIST"
lID = Nz(listSUP.Value, 0)


If (lstTASKS.ListCount <> 0) Then
    i = lstTASKS.ListCount - 1 'zero base
                 
    'select only values in listbox that match the current tasks assigned
    'do backwards so listbox finishes with scrollbar up at top
    Do Until (i = -1)
        taskID = lstTASKS.Column(0, i)
        bSel = lstTASKS.Selected(i)
        Call taskListGen1(taskID, lID, bSel)
        i = i - 1
    Loop
End If

End Sub

Private Sub taskListGen1(taskID As Long, supID As Long, bSelect As Boolean)
'needs taskID, supervisor ID, and listbox selection boolean

   Dim db As DAO.Database
   Dim rs As DAO.Recordset
   Dim childRS As Recordset
   Dim sqlString As String
     
   Set db = CurrentDb()
   Set rs = db.OpenRecordset("tblTASKLIST")
   rs.MoveFirst
     
   Do Until rs.EOF
      ' loop through only where taskID matches current iteration
        If rs!ID.Value = taskID Then
         ' Open a Recordset for the multivalued field.
            Set childRS = rs!supervisorResponsible.Value
    
            ' Exit if multivalued field contains no records.
            If (childRS.RecordCount <> 0) Then
                childRS.MoveFirst
                        
                ' Loop through the records in the child recordset.
                Do Until childRS.EOF
                   'see if supervisor matches the updated value
                    If (supID = childRS!Value.Value) And (bSelect = False) Then
                        'supervisor was once on this list, but no longer wants alerts
                        'sqlString = "DELETE childRS!Value.Value " & vbCr & _
                            "FROM childRS " & vbCr & _
                            "WHERE value =  " & supID & ";"
                        'db.Execute sqlString, dbFailOnError
                        childRS.Delete
                        rs.Close
                        db.Close
                        Exit Sub
                    ElseIf (supID = childRS!Value.Value) And (bSelect = True) Then
                        'supervisor is already on the list and assigned this task
                        rs.Close
                        db.Close
                        Exit Sub
                    End If
                    childRS.MoveNext
                Loop
                
                If (bSelect = True) Then
                        'supervisor being added to the list with others
                        rs.Edit
                        childRS.AddNew
                        childRS!Value.Value = supID
                        childRS.Update
                        rs.Update
                        rs.Close
                        db.Close
                        Exit Sub
                End If
                
            ElseIf (bSelect = True) And (childRS.RecordCount = 0) Then
                'if no supervisors are currently responsible, but you want to add a supervisor to this task,
                rs.Edit
                childRS.AddNew
                childRS!Value.Value = supID
                childRS.Update
                rs.Update
                Exit Do
            End If

            Exit Do
         End If
      rs.MoveNext
   Loop
   
   rs.Close
   db.Close
   
End Sub

