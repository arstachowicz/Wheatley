Public Sub condFormOpenPass(passID As Long)

'browse and create a duplicate entry if requested from another form
Dim db As DAO.Database
Dim rs As DAO.Recordset

Dim bRD As Boolean

Set db = CurrentDb
Set rs = db.OpenRecordset(tbl)

If (passID <> 0) Then

    cmbTALON.Enabled = True
    With rs
        .MoveFirst
        Do While Not .EOF
            If !ID = passID Then
                'pull boolean information
                    bRD = ![R&D]

                'Pull table information
                    cmbTALON = !partNum
                    cmbFixture = Nz(!fixtureNum)
                    cmbFlange1 = Nz(!maniNum)
                    cmbFLANGE2 = Nz(!maniNum1)
                    cmbFLANGE3 = Nz(!maniNum2)

                    
                'adjust toggle buttons
                    ckRD = bRD
                    Call tglCapt(Me.ckRD, 1)
                    
                'file and pages
                    txtFILE = Nz(!fileName)
                    txtPage = Nz(!pageNum)
                    Exit Do
            End If
            .MoveNext
        Loop
    End With

    cmbRev.SetFocus
Else:
    cmbTALON.Enabled = False
End If

    rs.Close
    db.Close

End Sub
