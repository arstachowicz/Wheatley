    cmbFRAME2 = Null
    cmbELEC1 = Null
    cmbELEC2 = Null
    txtVolt = Null
    txtTime = Null

End Sub

Private Sub List317_AfterUpdate()
Dim db As DAO.Database
Dim rs As DAO.Recordset

Dim bRD As Boolean
Dim bObs As Boolean

    Call clearAll
    bathID = CLng(Nz(List317, 0))
    sBath = "tblEP_BATHE"
    sP = ", "

Set db = CurrentDb
Set rs = db.OpenRecordset(sBath)

With rs
    .MoveFirst
    Do While Not .EOF
        If !ID = bathID Then
            'pull boolean information
                bRD = ![R&D]
                
                bObs = !obsolete
            
            'Pull table information
                cmbPARTNUM = !partNum
                cmbRev = !Rev
                tglRD = bRD
                tglObso = bObs
                txtFILE = Nz(!processlayout)
                txtPage = Nz(!pageNum)
                cmbASSEM = Nz(!assemblyID)
                cmbFRAME1 = Nz(!base1)
                cmbFRAME2 = Nz(!base2)
                cmbELEC1 = Nz(!grndelec1)
                cmbELEC2 = Nz(!grndElec2)
                txtVolt = Nz(!epVolt)
                txtTime = Nz(!epTime)
                Exit Do
        End If
        .MoveNext
    Loop
    .Close
End With

    'change captions of toggle buttons
    Call tglCapt(Me.tglRD, 1)
    Call tglCapt(Me.tglObso, 2)
    
    db.Close
End Sub
