Private Sub Form_Load()
    cmbTALON = Null
    cmbRev = Null

    cmbFix = Null
    cmbFlange1 = Null
    cmbFLANGE2 = Null
    cmbFLANGE3 = Null
    txtNotes = Null
    txtFILE = Null
    txtPage = Null
    
    tglObso = False
    tglRD = False
    Call tglCapt(Me.tglRD, 1)
    Call tglCapt(Me.tglObso, 2)
                
End Sub

Private Sub List651_AfterUpdate()
    Dim db As DAO.Database
    Dim rs As DAO.Recordset
    Dim tbl As String
    
    tbl = "tblPASSIVATE"
    passID = CLng(Nz(Me.List651, 0))

    Set db = CurrentDb
    Set rs = db.OpenRecordset(tbl)
 
    With rs
        .MoveFirst
        Do While Not .EOF
            If (!ID = passID) Then
                cmbTALON = !partNum
                cmbRev = !Rev
                cmbFlange1 = ![maniNum]
                cmbFLANGE2 = ![maniNum1]
                cmbFLANGE3 = ![maniNum2]
                cmbFix = ![fixtureNum]
                txtFILE = !fileName
                txtPage = !pageNum
                txtNotes = ![Notes]
                tglObso = !obsolete
                tglRD = ![R&D]
                Call tglCapt(Me.tglRD, 1)
                Call tglCapt(Me.tglObso, 2)
                Exit Do
            End If
            .MoveNext
        Loop
        .Close
    End With
    
    If (IsNull(txtFILE) = False) Then
        cmdView.Enabled = True
    Else:
        cmdView.Enabled = False
    End If
    
    db.Close
    
End Sub
